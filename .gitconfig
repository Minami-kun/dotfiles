[alias]
	co = checkout
	st = status
	ci = commit -a
	di = diff
	br = branch
	lo = log --graph --color --decorate
	sd = stash drop
	sl = stash list
	sp = stash pop
	ss = stash save
	delete-merged-branches = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
	rebase-master = rebase master

